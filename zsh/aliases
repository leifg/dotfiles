alias be="bundle exec"
alias mtf="git log --pretty=format: --name-only | grep -v '^$' | sort | uniq -c | sort -rg"
alias startmux="tmuxp load ~/.tmuxp/*.yml"
alias brewall='brew update && brew upgrade && brew bundle --global --no-lock && brew cu --all --yes; brew cleanup; date'
alias python="`which python3`"
alias pip="`which pip3`"