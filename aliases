alias be="bundle exec"
alias mtf="git log --pretty=format: --name-only | grep -v '^$' | sort | uniq -c | sort -rg"
alias solaris_project="git init; git config --add hub.host github.solarisbank.de; git config user.email leif.gensert.extern@solarisbank.de; git config user.signingkey 2BCE0BAD"
alias dcu="docker-compose up -d --build"
alias dcl="docker-compose logs -t --follow --tail=500"
alias startmux="tmuxp load ~/.tmuxp/*.yml"
alias debux="hyper run -e HOST=$(hyper ps | grep release_ping | awk '{print $1}') -e APP=release_ping -e NODE_COOKIE=\"${NODE_COOKIE}\" --rm -it leifg/debux"
